<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.github.clans.fab.FloatingActionMenu
        android:id="@+id/release_FAM"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|end"
        fab:menu_icon="@mipmap/release"
        fab:menu_showShadow="true"
        fab:menu_shadowColor="#66000000"
        fab:menu_colorNormal="@color/transparent"
        fab:menu_colorPressed="@color/transparent"
        android:layout_marginBottom="20dp"
        >
        <com.github.clans.fab.FloatingActionButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            fab:fab_showShadow="true"
            fab:fab_shadowColor="#66000000"
            fab:fab_colorNormal="@color/fab1_color"
            fab:fab_colorPressed="@color/transparent"
            ></com.github.clans.fab.FloatingActionButton>
        <com.github.clans.fab.FloatingActionButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            fab:fab_showShadow="true"
            fab:fab_shadowColor="#66000000"
            fab:fab_colorNormal="@color/fab2_color"
            fab:fab_colorPressed="@color/transparent"
            ></com.github.clans.fab.FloatingActionButton>
        <com.github.clans.fab.FloatingActionButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            fab:fab_showShadow="true"
            fab:fab_shadowColor="#66000000"
            fab:fab_colorNormal="@color/fab3_color"
            fab:fab_colorPressed="@color/transparent"
            ></com.github.clans.fab.FloatingActionButton>



    </com.github.clans.fab.FloatingActionMenu>

</FrameLayout>
